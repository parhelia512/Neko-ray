<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_checked="true">
        <layer-list>
            <item
                android:left="2.0dip"
                android:top="2.0dip"
                android:right="2.0dip"
                android:bottom="2.0dip">
                <shape
                    android:shape="rectangle">
                    <solid
                        android:color="?attr/colorOnSecondary" />
                    <corners
                        android:radius="26.0dip" />
                    <size
                        android:height="25.0dip"
                        android:width="25.0dip" />
                </shape>
            </item>
        </layer-list>
    </item>
    <item
        android:state_checked="false">
        <layer-list>
            <item
                android:left="2.0dip"
                android:top="2.0dip"
                android:right="2.0dip"
                android:bottom="2.0dip">
                <shape
                    android:shape="rectangle">
                    <solid
                        android:color="?attr/colorOutline" />
                    <corners
                        android:radius="26.0dip" />
                    <size
                        android:height="25.0dip"
                        android:width="25.0dip" />
                </shape>
            </item>
        </layer-list>
    </item>
</selector>